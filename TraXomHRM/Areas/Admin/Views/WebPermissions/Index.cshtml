@model IEnumerable<TraXomHRM.Areas.Admin.Models.DataModel.WebPermission>



<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.WebBusiness.BusinessName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PermissionName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.WebBusiness.BusinessName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PermissionName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.PermissionId }) |
                @Html.ActionLink("Details", "Details", new { id=item.PermissionId }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.PermissionId })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
